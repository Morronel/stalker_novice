msgid "ACTIVE"
msgstr "АКТИВНА"

#: templates/locations/garage.html:19
msgid "Access:"
msgstr "Доступ:"

#: templates/locations/garage.html:19
msgid "GRANTED"
msgstr "НАДАНО"

#: templates/locations/garage.html:21
